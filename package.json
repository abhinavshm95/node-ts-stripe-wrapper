{
    "name": "node-ts-stripe-wrapper",
    "version": "0.1.0",
    "scripts": {
        "build-ts": "tsc",
        "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
        "serve-debug": "nodemon --inspect dist/server.js",
        "build-serve": "npm run build-ts && npm run start",
        "serve": "node dist/server.js",
        "start": "npm run serve",
        "watch-node": "nodemon dist/server.js"
    },
    "dependencies": {
        "dotenv": "^8.2.0",
        "errorhandler": "1.5.1",
        "express": "4.17.1",
        "stripe": "^9.11.0",
        "winston": "3.3.3"
    },
    "devDependencies": {
        "@types/errorhandler": "1.5.0",
        "@types/express": "4.17.11",
        "@types/node": "^14.18",
        "@types/winston": "2.4.4",
        "@typescript-eslint/eslint-plugin": "4.14.2",
        "@typescript-eslint/parser": "4.14.2",
        "eslint": "7.19.0",
        "nodemon": "^2.0.7",
        "typescript": "4.1.3"
    },
    "engines": {
        "node": ">=14.0.0"
    }
}
